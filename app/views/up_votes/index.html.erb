<p id="notice"><%= notice %></p>

<h1>Up Votes</h1>

<table>
  <thead>
    <tr>
      <th>Picture</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @up_votes.each do |up_vote| %>
      <tr>
        <td><%= up_vote.picture %></td>
        <td><%= up_vote.user %></td>
        <td><%= link_to 'Show', up_vote %></td>
        <td><%= link_to 'Edit', edit_up_vote_path(up_vote) %></td>
        <td><%= link_to 'Destroy', up_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Up Vote', new_up_vote_path %>
